{
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "runtime": "nodejs18.x"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/main.js"
        }
    ]
}